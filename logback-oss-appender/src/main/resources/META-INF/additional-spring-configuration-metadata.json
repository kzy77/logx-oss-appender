{
  "properties": [
    {
      "name": "logging.logback.oss.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用OSS Appender",
      "defaultValue": false
    },
    {
      "name": "logging.logback.oss.endpoint",
      "type": "java.lang.String",
      "description": "S3兼容存储的endpoint URL"
    },
    {
      "name": "logging.logback.oss.region",
      "type": "java.lang.String",
      "description": "存储区域"
    },
    {
      "name": "logging.logback.oss.access-key-id",
      "type": "java.lang.String",
      "description": "访问密钥ID"
    },
    {
      "name": "logging.logback.oss.access-key-secret",
      "type": "java.lang.String",
      "description": "访问密钥Secret"
    },
    {
      "name": "logging.logback.oss.bucket",
      "type": "java.lang.String",
      "description": "存储桶名称"
    },
    {
      "name": "logging.logback.oss.key-prefix",
      "type": "java.lang.String",
      "description": "对象存储key前缀",
      "defaultValue": "logs/"
    },
    {
      "name": "logging.logback.oss.max-queue-size",
      "type": "java.lang.Integer",
      "description": "队列最大大小",
      "defaultValue": 200000
    },
    {
      "name": "logging.logback.oss.max-batch-count",
      "type": "java.lang.Integer",
      "description": "批处理最大条数",
      "defaultValue": 5000
    },
    {
      "name": "logging.logback.oss.max-batch-bytes",
      "type": "java.lang.Integer",
      "description": "批处理最大字节数",
      "defaultValue": 4194304
    },
    {
      "name": "logging.logback.oss.flush-interval-ms",
      "type": "java.lang.Long",
      "description": "强制刷新间隔毫秒数",
      "defaultValue": 2000
    },
    {
      "name": "logging.logback.oss.drop-when-queue-full",
      "type": "java.lang.Boolean",
      "description": "队列满时是否丢弃新日志",
      "defaultValue": false
    },
    {
      "name": "logging.logback.oss.multi-producer",
      "type": "java.lang.Boolean",
      "description": "是否支持多生产者",
      "defaultValue": false
    },
    {
      "name": "logging.logback.oss.max-retries",
      "type": "java.lang.Integer",
      "description": "最大重试次数",
      "defaultValue": 5
    },
    {
      "name": "logging.logback.oss.base-backoff-ms",
      "type": "java.lang.Long",
      "description": "基础退避时间毫秒数",
      "defaultValue": 200
    },
    {
      "name": "logging.logback.oss.max-backoff-ms",
      "type": "java.lang.Long",
      "description": "最大退避时间毫秒数",
      "defaultValue": 10000
    },
    {
      "name": "logging.logback.oss.pattern",
      "type": "java.lang.String",
      "description": "日志格式模板",
      "defaultValue": "%d{ISO8601} [%t] %-5level %logger{36} - %msg%n"
    },
    {
      "name": "logging.logback.oss.level",
      "type": "java.lang.String",
      "description": "日志级别",
      "defaultValue": "INFO"
    }
  ]
}